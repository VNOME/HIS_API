package core.classes.laboratory;

import java.util.Date;

import core.classes.api.admin.Employee;
import core.classes.api.user.User;

public class BloodPictureResult {

	/*
	 * This class corresponds to db table labTestResult_BloodPicture by
	 * MR
	 */

	int bpReportID;
	LabOrder bpOrderID;
	Employee bpDoneBy;
	Employee bpCheckedBy;
	Lab labID;
	String bpRemarks;
	
	Date bpCompletedDate;
	User bpCreateUser;
	User bpLastUpdateUser;
	Date bpCreateDate;
	Date bpLastUpdate;

	// result values relevant to lab test
	String bloodPicture;

	
	
	public int getBpReportID() {
		return bpReportID;
	}

	public void setBpReportID(int bpReportID) {
		this.bpReportID = bpReportID;
	}

	public LabOrder getBpOrderID() {
		return bpOrderID;
	}

	public void setBpOrderID(LabOrder bpOrderID) {
		this.bpOrderID = bpOrderID;
	}

	public Employee getBpDoneBy() {
		return bpDoneBy;
	}

	public void setBpDoneBy(Employee bpDoneBy) {
		this.bpDoneBy = bpDoneBy;
	}

	public Employee getBpCheckedBy() {
		return bpCheckedBy;
	}

	public void setBpCheckedBy(Employee bpCheckedBy) {
		this.bpCheckedBy = bpCheckedBy;
	}

	public String getBpRemarks() {
		return bpRemarks;
