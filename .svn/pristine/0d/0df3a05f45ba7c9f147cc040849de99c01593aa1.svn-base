package core.classes.api.user;

import java.util.Set;

/***
 * UserRoles Class contains all the attributes and there getters and setters. 
 * @author MIYURU
 * Version 1.0
 */
public class UserRoles {
	
	private int userRoleID;
	private String userRoleName;
	private Set<Permission> permissions; 
	
	/***
	 * Class Constructor
	 */
	public UserRoles(){
		
	}

/***
 * Get User Role ID
 * @return An Integer Value
 */
	public int getUserRoleID() {
		return userRoleID;
	}

/***
 * Set User Role ID
 * @param userRoleID An Integer Value
 */
	public void setUserRoleID(int userRoleID) {
		this.userRoleID = userRoleID;
	}

/***
 * Get User Role Name
 * @return A String Value
 */
	public String getUserRoleName() {
		return userRoleName;
	}

/**
 * Set User Role Name 
 * @param userRoleName A String value
 */
	public void setUserRoleName(String userRoleName) {
		this.userRoleName = userRoleName;
	}

/***
 * Get Role Permission
 * @return A Permission Type Set
 */
	public Set<Permission> getPermissions() {
		return permissions;
	}

/***
 * Set Role Permissions
 * @param permissions A Permission type Set
 */
	public void setPermissions(Set<Permission> permissions) {
		this.permissions = permissions;
	}	
	

	
	

}
