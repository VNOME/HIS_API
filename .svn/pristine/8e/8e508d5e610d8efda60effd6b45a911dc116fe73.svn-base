package core.classes.api.standards.icd10;

import java.util.Date;



/***
 * Icd10 Class contains all the attributes and there getters and setters
 * @author IMANKA
 * version 1.0
 *
 */

public class Icd10 {
	

	private int cIcdId;
	private String cIcdCode;
	private String cIcdName;
	private String cRemarks;
	private Date cCreateDate;
	private String cCreateUser;
	private Date cLastUpdate;
	private String cLastUpdateUser;
	private String cImmrCode;
	//private Immr cImmrCodeObj;
	
	/***
	 * Class Default Constructor. 
	 */
	public Icd10()
	{
		
	}
	
	public Icd10(int pIcdId,String pIcdCode, String pIcdName, String pRemarks, Date pCreateDate, String pCreateUser, Date pLastUpdate, String pLastUpdateUser,String pImmrCode)
	{
		this.cIcdId=pIcdId;
		this.cIcdCode=pIcdCode;
		this.cIcdName=pIcdName;
		this.cRemarks=pRemarks;
		this.cCreateDate=pCreateDate;
		this.cCreateUser=pCreateUser;
		this.cLastUpdate=pLastUpdate;
		this.cLastUpdateUser=pLastUpdateUser;
		this.cImmrCode=pImmrCode;
	}
	
	/***
	 * Get CIC ID
	 * @return A Integer Value
	 */
	public int getcIcdId() {
		return cIcdId;
	}
	
	/***
	 * Set CIC ID
	 * @param cIcdId A Integer Value
	 */
	public void setcIcdId(int cIcdId) {
		this.cIcdId = cIcdId;
	}
	
	/***
	 * Get CICCDCode
	 * @return a String Value
	 */
	public String getcIcdCode() {
		return cIcdCode;
	}
	
	/***
	 * Set CICCDCode
	 * @param cIcdCode a String Value
	 */
	public void setcIcdCode(String cIcdCode) {
		this.cIcdCode = cIcdCode;
	}
	
	/***
	 * Get a CIC Name
	 * @return a String Value
	 */
	public String getcIcdName() {
		return cIcdName;
	}
	
	/***
	 * Set CICD NAme
	 * @param cIcdName a String Value
	 */
	public void setcIcdName(String cIcdName) {
		this.cIcdName = cIcdName;
	}
	
	/***
	 * Get Remarks
	 * @return A String Value
	 */
	public String getcRemarks() {
		return cRemarks;
	}
	
	/***
	 * Set Remarks
	 * @param cRemarks A String Value
	 */
	public void setcRemarks(String cRemarks) {
		this.cRemarks = cRemarks;
	}
	
	/***
	 * Get Immmr Code
	 * @return A String Value
	 */
	public String getcImmrCode() {
		return cImmrCode;
	}

	/***
	 * Set IMMR Code
	 * @param cImmrCode A String Value
 	 */
	public void setcImmrCode(String cImmrCode) {
		this.cImmrCode = cImmrCode;
	}
	
	/***
	 * get Create Date
	 * @return A Date Value
	 */
	public Date getcCreateDate() {
		return cCreateDate;
	}
	
	/***
	 * Set Create Date
	 * @param cCreateDate
	 */
	public void setcCreateDate(Date cCreateDate) {
		this.cCreateDate = cCreateDate;
	}
	
	/***
	 * Get Create User
	 * @return a String Value
	 */
	public String getcCreateUser() {
		return cCreateUser;
	}
	
	/***
	 * Set Create User
	 * @param cCreateUser A String value
	 */
	public void setcCreateUser(String cCreateUser) {
		this.cCreateUser = cCreateUser;
	}
	public Date getcLastUpdate() {
		return cLastUpdate;
	}
	public void setcLastUpdate(Date cLastUpdate) {
		this.cLastUpdate = cLastUpdate;
	}
	public String getcLastUpdateUser() {
		return cLastUpdateUser;
	}
	public void setcLastUpdateUser(String cLastUpdateUser) {
		this.cLastUpdateUser = cLastUpdateUser;
	}

	

	
	
	

}
