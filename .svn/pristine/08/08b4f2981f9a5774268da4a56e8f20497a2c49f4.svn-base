package core.classes.api.user;

import java.util.Set;

import core.classes.api.admin.Employee;


/**
 * User Class contains all the attributes and there getters and setters.  
 * @author MIYURU
 * version 1.0
 *
 */

public class User {
	
	private int userID;
	private String userName;
	private String userPassword;
	private Employee employees;
	private UserRoles userRoles;	
	private Set<Permission> specialPermissions;

	
	/***
	 * Class Constructor.
	 */
	public User() {		
	}

/***
 * Get User ID
 * @return A Integer Value
 */
	public int getUserID() {
		return userID;
	}

/***
 * Set User ID
 * @param userID A Integer Value
 */
	public void setUserID(int userID) {
		this.userID = userID;
	}

/***
 * Get User Name
 * @return A String Value
 */
	public String getUserName() {
		return userName;
	}

/***
 * Set User Name
 * @param userName A String Value
 */
	public void setUserName(String userName) {
		this.userName = userName;
	}

/***
 * Get User Password
 * @return A String Value
 */
	public String getUserPassword() {
		return userPassword;
	}

/***
 * Set User Password
 * @param userPassword A String Value
 */
	public void setUserPassword(String userPassword) {
		this.userPassword = userPassword;
	}

/***
 * GEt User Role Object
 * @return A UserRole type Object
 */
	public UserRoles getUserRoles() {
		return userRoles;
	}

/***
 * Set User Role 
 * @param userRoles A User Role Object
 */
	public void setUserRoles(UserRoles userRoles) {
		this.userRoles = userRoles;
	}

/***
 * Get Employee Object 
 * @return employee A Employee Type Object
 */
	public Employee getEmployees() {
		return employees;
	}

/***
 * Set Employee Object
 * @param employees  A Employee Type Object
 */
	public void setEmployees(Employee employees) {
		this.employees = employees;
	}

/***
 * Get Special Permission For A User
 * @return A Permission type Set 
 */
	public Set<Permission> getSpecialPermissions() {
		return specialPermissions;
	}

/***
 * Set Get Special Permission For A User
 * @param specialPermissions A Permission type Set 
 */
	public void setSpecialPermissions(Set<Permission> specialPermissions) {
		this.specialPermissions = specialPermissions;
	}


	


	
	
	


	
	
}
